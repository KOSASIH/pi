# .env.example

# Network Configuration
# Specify the blockchain network to connect to (e.g., mainnet, testnet)
NETWORK=mainnet

# Infura Project ID
# Replace with your Infura project ID for Ethereum network access
INFURA_PROJECT_ID=your_infura_project_id

# Private Key
# Your wallet's private key (keep this secret and secure)
PRIVATE_KEY=your_wallet_private_key

# Database Configuration
# Database connection string (if applicable)
DATABASE_URL=mongodb://username:password@localhost:27017/pi_database

# AI Service Configuration
# API key for AI services (if applicable)
AI_SERVICE_API_KEY=your_ai_service_api_key

# Quantum Service Configuration
# Configuration for quantum computing services (if applicable)
QUANTUM_SERVICE_URL=https://quantum.example.com/api
QUANTUM_API_KEY=your_quantum_api_key

# Logging Configuration
# Set the logging level (e.g., debug, info, warn, error)
LOG_LEVEL=info

# Other Configuration
# Any other environment variables specific to your application
